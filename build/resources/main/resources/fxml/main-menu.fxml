<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="/resources/styles/styles.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="178.0" layoutY="60.0" text="Tic Tac Toe">
         <font>
            <Font size="80.0" />
         </font></Label>
      <Button layoutX="250.0" layoutY="220.0" mnemonicParsing="false" onAction="#handleHumanVsHuman" prefHeight="27.0" prefWidth="300.0" text="Human vs Human">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="red" />
            <String fx:value="big" />
         </styleClass></Button>
      <Button layoutX="252.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleHumanVsAi" prefHeight="27.0" prefWidth="300.0" text="Human vs Ai">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="red" />
            <String fx:value="big" />
         </styleClass></Button>
      <Button layoutX="252.0" layoutY="489.0" mnemonicParsing="false" onAction="#handleAiVsAi" prefHeight="27.0" prefWidth="300.0" text="Ai vs Ai">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="red" />
            <String fx:value="big" />
         </styleClass></Button>
   </children>
</AnchorPane>
