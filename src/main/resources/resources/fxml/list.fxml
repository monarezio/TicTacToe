<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="/resources/styles/styles.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <SplitPane dividerPositions="0.5" prefHeight="540.0" prefWidth="800.0" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <items>
          <AnchorPane fx:id="circle" id="circle" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <ListView fx:id="listCircle" onMouseClicked="#handleCircle" layoutY="48.0" prefHeight="470.0" prefWidth="395.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="48.0" />
                  <Label layoutX="14.0" layoutY="14.0" prefHeight="17.0" prefWidth="363.0" text="Circle AI" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
          <AnchorPane fx:id="cross" id="cross" minHeight="0.0" minWidth="0.0" prefHeight="165.0" prefWidth="145.0">
               <children>
                  <ListView fx:id="listCross" onMouseClicked="#handleCross" layoutY="49.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="48.0" />
                  <Label layoutX="16.0" layoutY="14.0" prefHeight="17.0" prefWidth="363.0" text="Cross AI" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
       <Button layoutX="34.0" layoutY="542.0" onAction="#handleBack" mnemonicParsing="false" prefHeight="27.0" prefWidth="99.0" text="Back">
           <styleClass>
               <String fx:value="btn" />
               <String fx:value="red" />
           </styleClass>
       </Button>
      <Button fx:id="continueButton" onAction="#handleContinue" layoutX="603.0" disable="true" layoutY="542.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="151.0" text="Continue">
        <styleClass>
          <String fx:value="btn" />
          <String fx:value="blue" />
        </styleClass>
      </Button>
   </children>
</AnchorPane>
